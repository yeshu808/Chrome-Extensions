{
    "manifest_version": 3,
    "name": "Must watch Movies",
    "version": "1.0",
    "description": "This extension tracks list of must watch movies",
    "action": {
        "default_icon": "movie.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "eventPage.js"
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ]
}